{
  "name": "zirkeldesign/sage-blade-icons",
  "type": "package",
  "license": "MIT",
  "description": "Use blade-ui-kit/blade-icons in roots/sage themes.",
  "version": "0.1.1",
  "authors": [
    {
      "name": "Daniel Sturm",
      "email": "d.sturm@zirkeldesign.de"
    }
  ],
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "ZD\\SageBladeIcons\\": "src/"
    }
  },
  "require": {
    "php": "^7.3|^8.0",
    "blade-ui-kit/blade-icons": "^1.0"
  },
  "require-dev": {
    "roots/acorn": "dev-main"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
